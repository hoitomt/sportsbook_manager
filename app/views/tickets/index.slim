.page-header
  h1 History
= @test_data
- unless @tickets.nil?
  .row
    .col-md-10
      .col-md-1
        .results-heading ID
      .col-md-1
        .results-heading Date
      .col-md-2
        .results-heading Type
      .col-md-2
        .results-heading Wagered
      .col-md-2
        .results-heading To Win
      .col-md-2
        .results-heading Outcome
  .row
    - @tickets.each do |ticket|
      .col-md-10
        .col-md-1
          .result==ticket.sb_bet_id
        .col-md-1
          .result==ticket.display_date
        .col-md-2
          .result==ticket.type
        .col-md-2
          .result==ticket.amount_wagered
        .col-md-2
          .result==ticket.amount_to_win
        .col-md-2
          .result==ticket.outcome
        .row
          .col-md-10.col-md-offset-1
            == slim :'tickets/ticket_line_items', locals: {ticket_line_items: ticket.ticket_line_items}

  .row
    table.tickets
      tr
        th ID
        th Date
        th Type
        th Amount Wagered
        th Amount To Win
        th Outcome
      - @tickets.each do |ticket|
        tr
          td=ticket.sb_bet_id
          td=ticket.display_date
          td=ticket.type
          td=ticket.amount_wagered
          td=ticket.amount_to_win
          td=ticket.outcome
        == slim :'tickets/ticket_line_items', locals: {ticket_line_items: ticket.ticket_line_items}

